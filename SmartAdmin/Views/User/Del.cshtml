@{
    ViewBag.Title = "删除用户";
}

<!-- Page title -->
<div id="pagetitle">
    <div class="wrapper">
        <h1>用户管理 &rarr; <span>删除用户</span></h1>
        <!-- Quick search box -->
        <form action="" method="get">
            <input class="" type="text" id="q" name="q" value="请输入搜索关键字" />
        </form>
    </div>
</div>
<!-- End of Page title -->
<!-- Page content -->
<div id="page">
    <!-- Wrapper -->
    <div class="wrapper">
        <!-- 左边分部小节开始 -->
        <section class="column width6 first">
            <h3>功能简介</h3>
            <div class="box box-info">
                用户信息删除提示。
            </div>

            <!--正文内容部分开始-->
            提示：@ViewBag.Message
            <a href="@Url.Content("~/User/Index")">点击返回</a>
            <!--正文内容部分结束-->

        </section>
        <!-- 左边分部小节结束 -->



        <!-- 右边分部小节开始 -->
        <aside class="column width2">
            @Html.Partial("_QuickMenu");
            <div class="content-box">
                <header>
                    <h3>便捷信息</h3>
                </header>
                <section>
                    <q>这里描述一段实用的便捷描述文字信息。
                    <cite>w3schools.com</cite></q>
                </section>
            </div>
        </aside>
        <!-- 右边分部小节结束 -->
    </div>
    <!-- End of Wrapper -->
</div>
<!-- End of Page content -->
